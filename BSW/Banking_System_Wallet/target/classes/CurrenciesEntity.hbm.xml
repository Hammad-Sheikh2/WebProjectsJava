<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.sql.Modals.CurrenciesEntity" table="currencies" schema="banking_system_wallet">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="currName">
            <column name="curr_name" sql-type="varchar(255)"/>
        </property>
        <property name="currAnnotation">
            <column name="curr_annotation" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="currValue">
            <column name="curr_value" sql-type="double" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>